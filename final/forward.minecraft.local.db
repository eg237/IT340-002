;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.minecraft.local. root.minecraft.local. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	localhost.
@	IN	A	127.0.0.1
@	IN	AAAA	::1
;Name Server Information
@ IN NS ns1.minecraft.local.
;IP address of Name Server
ns1 IN A 192.168.1.197
;Mail Exchanger
minecraft.local. IN MS 10 mail.minecraft.local.
;A-Record HostName To Ip Address
www IN A 192.168.1.197
mail IN A 192.168.0.150
;CNAME record
ftp IN CNAME www.minecraft.local.
